{
  "manifest_version": 2,
  "name": "Safe Browsing Box",
  "version": "1.0.0",

  "default_locale": "en",
  "description": "Take control over your data privacy online with the Safe Browsing Box.",
  "author": "Lucy Walsh",
  "homepage_url": "https://github.com/lucywalsh/safebrowsingbox-ext",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "Safe Browsing Box",
    "default_popup": "popup/dashboard.html"
    "browser_style": true
 },
 "content_scripts": [
 {
    "matches": ["<all_urls>"],
    "js": ["content_scripts/analyse_site.js"],
    "run_at": "document_start"
  }
],
 "permissions": [
   "activeTab"
 ]
}
